<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ct.mapper.AuthorityMapper">
  <resultMap id="BaseResultMap" type="com.ct.pojo.Authority">
    <result column="a_id" jdbcType="INTEGER" property="aId" />
    <result column="a_role" jdbcType="VARCHAR" property="aRole" />
  </resultMap>
  <insert id="insert" parameterType="com.ct.pojo.Authority">
    insert into authority (a_id, a_role)
    values (#{aId,jdbcType=INTEGER}, #{aRole,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.ct.pojo.Authority">
    insert into authority
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="aId != null">
        a_id,
      </if>
      <if test="aRole != null">
        a_role,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="aId != null">
        #{aId,jdbcType=INTEGER},
      </if>
      <if test="aRole != null">
        #{aRole,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>