<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ct.mapper.SupplierMapper">
  <resultMap id="SuppliersMap" type="com.ct.pojo.Supplier">
    <result column="su_id" jdbcType="INTEGER" property="suId" />
    <result column="su_name" jdbcType="VARCHAR" property="suName" />
    <result column="su_phone" jdbcType="INTEGER" property="suPhone" />
    <result column="add_id" jdbcType="INTEGER" property="addId" />
  </resultMap>
  <insert id="insert" parameterType="com.ct.pojo.Supplier">
    insert into supplier (su_id, su_name, su_phone, 
      add_id)
    values (#{suId,jdbcType=INTEGER}, #{suName,jdbcType=VARCHAR}, #{suPhone,jdbcType=INTEGER}, 
      #{addId,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.ct.pojo.Supplier">
    insert into supplier
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="suId != null">
        su_id,
      </if>
      <if test="suName != null">
        su_name,
      </if>
      <if test="suPhone != null">
        su_phone,
      </if>
      <if test="addId != null">
        add_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="suId != null">
        #{suId,jdbcType=INTEGER},
      </if>
      <if test="suName != null">
        #{suName,jdbcType=VARCHAR},
      </if>
      <if test="suPhone != null">
        #{suPhone,jdbcType=INTEGER},
      </if>
      <if test="addId != null">
        #{addId,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>